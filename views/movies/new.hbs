<h2> </h2>
<form action="/movies" method="POST">
    <label>Title: </label>
    <input name="title">
    
    <label>Genre: </label>
    <input name="genre">

    <label>Plot: </label>
    <input name="plot">

    <label>Cast: 
        <select name="cast" multiple>
        {{#each celebrities}}
        <option value="{{id}}">{{name}}</option>
        {{/each}}
        </select>
    </label>

    <button>Create</button>
</form>